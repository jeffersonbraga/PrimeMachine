package br.com.opsocial.server.utils.reports.linkedin;

public class LinkedinFollowStatistics {
	
	private LinkedinObjects regions;
	private LinkedinObjects functions; //Area de atuação
	private LinkedinObjects industries; //Tipo de industria
	private Long count;
	private LinkedinObjects companySizes;
	private Long nonEmployeeCount;
	private Long employeeCount;
	private LinkedinObjects seniorities; //Posição do usuário

	public LinkedinObjects getRegions() {
		return regions;
	}

	public void setRegions(LinkedinObjects regions) {
		this.regions = regions;
	}

	public LinkedinObjects getFunctions() {
		return functions;
	}

	public void setFunctions(LinkedinObjects functions) {
		this.functions = functions;
	}

	public LinkedinObjects getIndustries() {
		return industries;
	}

	public void setIndustries(LinkedinObjects industries) {
		this.industries = industries;
	}

	public Long getCount() {
		return count;
	}

	public void setCount(Long count) {
		this.count = count;
	}

	public LinkedinObjects getCompanySizes() {
		return companySizes;
	}

	public void setCompanySizes(LinkedinObjects companySizes) {
		this.companySizes = companySizes;
	}

	public Long getNonEmployeeCount() {
		return nonEmployeeCount;
	}

	public void setNonEmployeeCount(Long nonEmployeeCount) {
		this.nonEmployeeCount = nonEmployeeCount;
	}

	public Long getEmployeeCount() {
		return employeeCount;
	}

	public void setEmployeeCount(Long employeeCount) {
		this.employeeCount = employeeCount;
	}

	public LinkedinObjects getSeniorities() {
		return seniorities;
	}

	public void setSeniorities(LinkedinObjects seniorities) {
		this.seniorities = seniorities;
	}

	public LinkedinFollowStatistics() {}
}
